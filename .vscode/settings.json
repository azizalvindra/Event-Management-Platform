{
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "peacock.color": "#55328e",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#6c3fb4",
    "activityBar.background": "#6c3fb4",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#c5835a",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#6c3fb4",
    "statusBar.background": "#55328e",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#6c3fb4",
    "statusBarItem.remoteBackground": "#55328e",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#55328e",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#55328e99",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
